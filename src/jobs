<script type="text/javascript">
 	 var stimedout = false;
 	 var keepMeLoggedIn;
	 	if (keepMeLoggedIn == null || keepMeLoggedIn == undefined) {
	        if (window.XMLHttpRequest) {
	        	keepMeLoggedIn = new XMLHttpRequest();
	        }
	        else if (window.ActiveXObject) {
	        	keepMeLoggedIn = new ActiveXObject("Microsoft.XMLHTTP");
	        }
	 }
	 keepMeLoggedIn.onreadystatechange = function () { };
 	 var interruptTimeout = false;
 	 var countdownNum = 29;
 	 var countdownInterval;
 	 var sessionInterval;
 	 
 	 orbisApp.setUpRegularDialog("#keepMeLoggedInDiv", {"Keep Me Logged In" : keepMeLoggedInClicked}, {width:400});
 	 
	 function sessionTimeoutPopup() {
		 interruptTimeout = false;
		 beginCountdown();
		 if (stimedout == false) {
			 $("#keepMeLoggedInDiv").dialog("open");
			 setTimeout ( "sessionTimeoutTimeout()", (30 * 1000));
		 }
	 }
	 
	 function beginCountdown()
	 {
		 countdownInterval = setInterval("countdown()", 1000);
	 }
	 
	 function countdown()
	 {
		 $("#secondsLeft").html(countdownNum);
		 countdownNum--;
	 }
	 
	 function stopCountdown()
	 {
		 $("#secondsLeft").html("30");
		 countdownNum = 29;
		 clearInterval(countdownInterval);
	 }
	 
	 function gotoMyAccount() {
		 document.location='/myAccount';
	 }
	 
	 function sessionTimeoutTimeout() {
	     if (!interruptTimeout) {
		 	stopCountdown();
	 		stimedout = true;
	 		$("span#timeoutMessage").html("Your session has expired. You have been logged out. <br /><a href='/myAccount'>Click here to log in</a>");
	 		$("#keepMeLoggedInDiv").dialog("option", "buttons", {"Close" : function(){$(this).dialog("close");}});
	 		keepMeLoggedIn.open('post', '/logMeOut.htm');
		 	keepMeLoggedIn.send(null);
	     }
	 }
	 
	 function keepMeLoggedInClicked() {
		 clearInterval(sessionInterval);
		 
	 	 keepMeLoggedIn.open('get', '/keepAlive.htm?rand=' + Math.floor(Math.random()*100000));
	 	 keepMeLoggedIn.send(null);
	 	
	 	$("#keepMeLoggedInDiv").dialog("close");
	 	sessionInterval = setInterval ( "sessionTimeoutPopup()", (1770 * 1000));
	 	stopCountdown();
	 	interruptTimeout = true;
	 }
	 $("#keepMeLoggedInDiv").dialog("close");
	 sessionInterval = setInterval ( "sessionTimeoutPopup()", (1770 * 1000));
