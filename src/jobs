<script type="text/javascript">
	orbisApp.addComponent('jqueryValidate');
	var saveSearchDialogButtons = {
		Save : function(){$("form#postingForm").submit();},
		Close : function(){$(this).dialog("close");}
	};

	$(document).ready(function() {
		
		orbisApp.setUpRegularDialog("div#saveSearchDialog", saveSearchDialogButtons);
		var inputErrorClass = "inputError";
		$("#postingForm").validate({
			highlight: function(element, errorClass) {
			     $(element).addClass(inputErrorClass);
			},
			unhighlight: function(element, errorClass) {
			     $(element).removeClass(inputErrorClass);
			}
		});
		
		loadPostingTableFor2();
	});
	
	function openSaveSearchDialog()
	{
		$("div#saveSearchDialog").dialog("open");
	}
	
	
</script>

